{
  "name": "@serlo/serlo.org-cloudflare-worker",
  "version": "0.0.0",
  "bugs": {
    "url": "https://github.com/serlo/serlo.org-cloudflare-worker/issues"
  },
  "repository": "serlo/serlo.org-cloudflare-worker",
  "license": "Apache-2.0",
  "author": "Serlo Education e.V.",
  "scripts": {
    "build": "wrangler build",
    "check:all": "scripts/run_all_checks.sh",
    "dev": "wrangler dev --env staging",
    "deploy": "wrangler publish",
    "format": "npm-run-all -c format:*",
    "format:eslint": "yarn _eslint --fix",
    "format:prettier": "yarn _prettier --write",
    "lint": "npm-run-all --parallel lint:prettier lint:tsc lint:eslint",
    "lint:eslint": "yarn _eslint",
    "lint:prettier": "yarn _prettier --check",
    "lint:tsc": "tsc --noEmit",
    "license": "ts-node scripts/license-headers",
    "start": "start-storybook -p 6006",
    "test": "jest",
    "test:staging": "cross-env TEST_ENVIRONMENT=staging jest --runInBand",
    "test:production": "cross-env TEST_ENVIRONMENT=production jest --runInBand",
    "wrangler:maintenance:kv:get": "wrangler kv:key get --binding=MAINTENANCE_KV",
    "wrangler:maintenance:kv:put": "wrangler kv:key put --binding=MAINTENANCE_KV",
    "wrangler:maintenance:kv:delete": "wrangler kv:key delete --binding=MAINTENANCE_KV",
    "maintenance": "ts-node scripts/maintenance.ts",
    "show-unused-exports": "ts-unused-exports tsconfig.json --excludePathsFromReport=__stories__ --excludePathsFromReport=__tests__/__utils__/index.ts",
    "_eslint": "eslint \"{{.storybook,__fixtures__,__stories__,__tests-pacts__,__tests__,scripts,src}/**/*,*}.{js,jsx,ts,tsx}\"",
    "_prettier": "prettier ."
  },
  "dependencies": {
    "core-js-pure": "^3.0.0",
    "fp-ts": "^2.0.0",
    "io-ts": "^2.0.0",
    "jsonwebtoken": "^8.0.0",
    "luxon": "^2.0.0",
    "marked": "^4.0.0",
    "preact": "^10.0.0",
    "preact-render-to-string": "^5.0.0",
    "ramda": "^0.27.0",
    "sanitize-html": "^2.0.0",
    "toucan-js": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@cloudflare/workers-types": "^3.0.0",
    "@cloudflare/wrangler": "^1.0.0",
    "@iarna/toml": "^2.0.0",
    "@splish-me/copyright-headers": "^0.0.2",
    "@storybook/preact": "^6.0.0",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/preact": "^2.0.0",
    "@types/core-js": "^2.0.0",
    "@types/iarna__toml": "^2.0.0",
    "@types/jest": "^26.0.0",
    "@types/jsonwebtoken": "^8.0.0",
    "@types/luxon": "^2.0.0",
    "@types/marked": "^4.0.0",
    "@types/node": "^16.0.0",
    "@types/ramda": "^0.27.0",
    "@types/rimraf": "^3.0.0",
    "@types/sanitize-html": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-loader": "^8.0.0",
    "cross-env": "^7.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-react": "^7.0.0",
    "glob": "^7.0.0",
    "jest": "^26.0.0",
    "msw": "^0.24.0",
    "node-fetch": "3.0.0-beta.9",
    "nodemon": "^2.0.0",
    "npm-run-all": "^4.0.0",
    "prettier": "^2.0.0",
    "prettier-plugin-sh": "^0.7.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^26.0.0",
    "ts-loader": "^8.0.0",
    "ts-node": "^9.0.0",
    "ts-unused-exports": "^7.0.0",
    "typescript": "^4.0.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.0.0"
  },
  "engines": {
    "node": "^16.0.0"
  },
  "private": true
}
