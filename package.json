{
  "name": "@serlo/serlo.org-cloudflare-worker",
  "version": "0.0.0",
  "bugs": {
    "url": "https://github.com/serlo/serlo.org-cloudflare-worker/issues"
  },
  "repository": "serlo/serlo.org-cloudflare-worker",
  "license": "Apache-2.0",
  "author": "Serlo Education e.V.",
  "scripts": {
    "build": "wrangler build",
    "dev": "wrangler dev --env dev",
    "deploy": "wrangler publish",
    "format": "npm-run-all -c format:*",
    "format:eslint": "yarn _eslint --fix",
    "format:prettier": "yarn _prettier --write",
    "lint": "npm-run-all --parallel lint:prettier lint:tsc lint:eslint",
    "lint:eslint": "yarn _eslint",
    "lint:prettier": "yarn _prettier --check",
    "lint:tsc": "tsc --noEmit",
    "license": "ts-node scripts/license-headers",
    "start": "start-storybook -p 6006",
    "test": "jest",
    "wrangler:maintenance:kv:get": "wrangler kv:key get --binding=MAINTENANCE_KV",
    "wrangler:maintenance:kv:put": "wrangler kv:key put --binding=MAINTENANCE_KV",
    "wrangler:maintenance:kv:delete": "wrangler kv:key delete --binding=MAINTENANCE_KV",
    "maintenance": "ts-node scripts/maintenance.ts",
    "_eslint": "eslint \"{{.storybook,__fixtures__,__stories__,__tests-pacts__,__tests__,scripts,src}/**/*,*}.{js,jsx,ts,tsx}\"",
    "_prettier": "prettier .",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@types/marked": "^0.7.0",
    "@types/sanitize-html": "^1.0.0",
    "jsonwebtoken": "^8.0.0",
    "luxon": "^1.0.0",
    "marked": "^0.8.0",
    "preact": "^10.0.0",
    "preact-render-to-string": "^5.0.0",
    "ramda": "^0.27.0",
    "sanitize-html": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@cloudflare/wrangler": "^1.0.0",
    "@splish-me/copyright-headers": "^0.0.2",
    "@storybook/preact": "^5.0.0",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/preact": "^1.0.0",
    "@types/jest": "^25.0.0",
    "@types/jsonwebtoken": "^8.0.0",
    "@types/luxon": "^1.0.0",
    "@types/node": "^12.0.0",
    "@types/node-fetch": "^2.0.0",
    "@types/ramda": "^0.27.0",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-loader": "^8.0.0",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-react": "^7.0.0",
    "glob": "^7.0.0",
    "jest": "^25.0.0",
    "node-fetch": "^2.0.0",
    "nodemon": "^2.0.0",
    "npm-run-all": "^4.0.0",
    "prettier": "^2.0.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.0.0",
    "ts-loader": "^6.0.0",
    "ts-node": "^8.0.0",
    "typescript": "^3.0.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.0.0"
  },
  "engines": {
    "node": "^12.0.0"
  },
  "private": true
}
