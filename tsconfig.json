{
  "compilerOptions": {
    "incremental": true,
    "outDir": "dist",
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "lib": ["es2021"],
    "jsx": "react",
    "jsxFactory": "h",
    "module": "commonjs",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": true,
    "target": "es2019",
    "types": ["@cloudflare/workers-types/2022-08-04"]
  }
}
